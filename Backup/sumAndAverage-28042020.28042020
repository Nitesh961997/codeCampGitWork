#!/bin/bash
array=( a b c d e );
for i in `a b c d e`;
#for i in "${array[@]}";
do
$i=$((RANDOM%100));
if [ 10 -gt $i ];
then
        $i=$(($i+10));
else
        $i=$i;
fi
echo $i;
done;
